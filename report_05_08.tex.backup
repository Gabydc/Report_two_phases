\end{equation}
\begin{equation}
 \frac{\partial(\phi \rho_{n}(1-{S}_{w}))}{\partial t}+\nabla \cdot ( \rho_{n} \frac{{K}k_{rn}}{\mu_n}(\nabla p_n-\rho_n g \nabla z))=\rho_{n} q_{n}.
\end{equation}

